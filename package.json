{
  "private": true,
  "name": "gyazz-aclone",
  "version": "0.0.1",
  "description": "convert gyazz-clone to slide",
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "main": "index.js",
  "scripts": {
    "browserify": "browserify --verbose -t [ babelify ] -g uglifyify src/index.js -o dist/index.js",
    "watchify": "watchify --verbose --debug -t [ babelify ] -p browserify-notify src/index.js -o dist/index.js",
    "push-gh-pages": "git status -s | grep '.' && echo 'you should commit before push-gh-pages' || (git add -f ./dist && git commit -m 'push gh-pages' && git push -f origin HEAD:gh-pages; git reset --hard HEAD^)",
    "server": "static-server",
    "start": "parallelshell 'npm run watchify' 'npm run server'"
  },
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-notify": "^1.1.2",
    "chokidar-cli": "^1.2.0",
    "parallelshell": "^2.0.0",
    "static-server": "^2.0.2",
    "uglifyify": "^3.0.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "jquery": "^3.0.0"
  }
}
